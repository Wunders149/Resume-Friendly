{
    "pythonVersion": "3.14",
    "pythonPlatform": "Windows",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.14",
            "extraPaths": [
                "${workspaceRoot}/src"
            ]
        }
    ],
    "include": [
        "src",
        ".",
        "*.py"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.*"
    ],
    "extraPaths": [
        "${workspaceRoot}/src"
    ],
    "venvPath": ".",
    "venv": "venv",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false
}
